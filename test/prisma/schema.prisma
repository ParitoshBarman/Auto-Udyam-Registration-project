generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Registration {
  id         Int      @id @default(autoincrement())
  step       Int
  formData   Json     // store entire submitted form as JSON
  createdAt  DateTime @default(now())
}
